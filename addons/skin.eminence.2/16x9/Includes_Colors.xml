<?xml version="1.0" encoding="UTF-8"?>
<includes>
    <include name="Colors_Panel">
        <control type="panel" id="$PARAM[id]">
            <left>bigpad</left>
            <right>bigpad</right>
            <bottom>bigpad</bottom>
            <top>80</top>
            <orientation>vertical</orientation>
            <nested />
            <itemlayout width="140" height="140">
                <control type="group">
                    <top>10</top>
                    <bottom>10</bottom>
                    <left>10</left>
                    <right>10</right>
                    <include>Object_FlatBackground</include>
                    <control type="image">
                        <texture background="true">$INFO[ListITem.Icon]</texture>
                        <bordersize>10</bordersize>
                    </control>
                </control>
            </itemlayout>
            <focusedlayout width="140" height="140">
                <control type="group">
                    <top>10</top>
                    <bottom>10</bottom>
                    <left>10</left>
                    <right>10</right>
                    <include>Object_FlatBackground</include>
                    <control type="image">
                        <texture background="true">$INFO[ListITem.Icon]</texture>
                        <bordersize>0</bordersize>
                    </control>
                </control>
            </focusedlayout>
            <content><include>Colors_Items</include></content>
        </control>
    </include>

    <include name="Colors_Dialog">
        <param name="id" default="9000" />
        <param name="visible" default="true" />
        <definition>
            <include>Animation_SlideIn</include>
            <include>Animation_FadeOut</include>
            <centerleft>50%</centerleft>
            <width>1160</width>
            <bottom>150</bottom>
            <height>520</height>
            <control type="group">
                <include>Dialog_Background</include>
            </control>
            <include content="Dialog_Top_IconHeader">
                <param name="label" value="$LOCALIZE[14078]" />
                <param name="icon" value="special://skin/extras/icons/filebox.png" />
            </include>
            <include content="Colors_Panel">
                <param name="id" value="$PARAM[id]" />
                <nested />
            </include>
        </definition>
    </include>

